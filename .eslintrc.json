{
	"extends": ["next", "prettier", "next/core-web-vitals"],
	"ignorePatterns": ["/app/code-examples/*"],
	"plugins": ["unused-imports"],
	"rules": {
		"unused-imports/no-unused-imports": "error",
		"react/jsx-curly-brace-presence": [
			"warn",
			{
				"props": "never",
				"children": "never"
			}
		]
	}
}
